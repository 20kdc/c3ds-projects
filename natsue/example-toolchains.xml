<?xml version="1.0" encoding="UTF-8"?>
<!-- Would be installed at ~/.m2/toolchains.xml -->
<toolchains>
	<toolchain>
		<type>jdk</type>
		<provides>
			<version>1.8</version>
			<vendor>openjdk</vendor>
		</provides>
		<configuration>
			<jdkHome>/usr/lib/jvm/java-8-openjdk-amd64</jdkHome>
		</configuration>
	</toolchain>
</toolchains>

